{
  "name": "web_scrapper",
  "version": "1.0.0",
  "description": "A test project using Puppeteer to scrap web pages.",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev --watch ['src'] --respawn src/index.ts",
    "lint": "eslint --ext .ts src tests",
    "prettier": "prettier '{src,tests}/**/*.ts' --write",
    "tsc": "tsc",
    "build": "rm -rf build && tsc --build tsconfig.build.json"
  },
  "keywords": [
    "Puppeteer",
    "scrap"
  ],
  "author": "Onubogu Chibuikem",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-typescript-airbnb-base": "^1.0.0-rc1",
    "eslint-plugin-import": "^2.27.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
